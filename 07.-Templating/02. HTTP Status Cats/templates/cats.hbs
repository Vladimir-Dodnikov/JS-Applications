<ul>
    {{#each cats}}
    <li>
        <img src="./images/{{imageLocation}}.jpg" width="250" height="250" alt="Card image cap">
        <div class="info">
            <button data-toggle="collapse" class="showBtn" onclick="toggle(this)">Show status code</button>
            <div class="status" style="display: none" id="{{this.id}}">
                <h4>Status Code: {{this.statusCode}}</h4>
                <p>{{this.statusMessage}}</p>
            </div>
        </div>
    </li>
    {{/each}}
</ul>